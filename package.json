{
	"name": "sveltekit-blog",
	"description": "A blog site made with SvelteKit.",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "git://github.com/emrecancorapci/sveltekit-blog.git"
	},
	"homepage": "https://github.com/emrecancorapci/sveltekit-blog",
	"main": "src/routes/+page.svelte",
	"scripts": {
		"dev": "vite dev --port 3300",
		"build": "vite build",
		"preview": "vite preview --port 3300",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.3.4",
		"bootstrap": "^5.2.3"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.3",
		"@sveltejs/kit": "^1.11.0",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.1.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	},
	"author": {
		"name": "Emre Can Çorapçı",
		"email": "emrecancorapci@gmail.com"
	},
	"license": "MIT"
}
